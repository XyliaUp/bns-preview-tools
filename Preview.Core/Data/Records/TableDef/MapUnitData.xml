			<case alias="classic-field" />
			<case alias="faction-battle-field" />
			<case alias="guild-battle-field" />
			<case alias="party-battle-startpoint" />
			<case alias="party-battle-enemy" />
			<case alias="fishing-field" />
        </record>
		<record alias="map-depth" type="Seq" >
			<case alias="1" default="y" />
			<case alias="2" />
			<case alias="3" />
			<case alias="4" />
			<case alias="5" />
		</record>
		<record alias="arena-dungeon-map-depth" type="Seq" >
			<case alias="1" default="y" />
			<case alias="2" />
			<case alias="3" />
			<case alias="4" />
			<case alias="5" />
		</record>
		<record alias="zoom" type="Bool" />
		<record alias="rotate" type="Bool" />		
		<record alias="click" type="Bool" />			
		<record alias="front" type="Bool" />			
		<record alias="show-tooltip" type="Bool" default="y" />	
		<record alias="name2" ref="Text" />
		<record alias="opacity" type="Int16" />
		<record alias="size-x" type="Int16" />
		<record alias="size-y" type="Int16" />
