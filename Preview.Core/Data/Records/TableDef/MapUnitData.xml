	<list module="ZoneData0" type="mapunit" version="1.78" rule="unsort" >

		<record alias="type" type="type" >
			<case alias="static" />
			<case alias="quest" />
			<case alias="link" />
			<case alias="npc" />
			<case alias="boss" />
			<case alias="airdash" />
			<case alias="env" />
			<case alias="attraction" />
			<case alias="npc-group" />
			<case alias="guild-battle-field-portal" />
			<case alias="party-battle-startpoint-alpha" />
			<case alias="party-battle-startpoint-beta" />
			<case alias="fishing-field" />			
		</record>

		<record alias="id" type="Int32" key="y" />

		<record alias="alias" type="String" />
		<record alias="mapid" type="Int16" />
		<record alias="zone-id" type="Int32" />
		<record alias="is-phasing-unit" type="Bool" />
		<record alias="position-x" type="Float32" />
		<record alias="position-y" type="Float32" />
