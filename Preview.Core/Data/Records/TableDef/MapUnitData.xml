		<record alias="link-map-location-y" type="Float32" filter="link" />
		<!-- =================================================================== -->

		<!-- ============================ [3=] 开始 ============================ -->
		<record alias="npc" filter="npc" ref="Npc" />
		<record alias="ignore-object-pos" type="Bool" filter="npc" />
		<record alias="in-sight-only" type="Bool" filter="npc" />
		<record alias="many-counts" type="Bool" filter="npc" />
		<!-- =================================================================== -->


		<!-- ========================== [4=] 开始 ========================== -->
		<record alias="npc" filter="boss" ref="Npc" />
		<record alias="combat-imageset" type="String" filter="boss" />
		<record alias="combat-over-imageset" type="String" filter="boss" />
		<record alias="combat-pressed-imageset" type="String" filter="boss" />
		<record alias="combat-outofsight-imageset" type="String" filter="boss" />
		<record alias="combat-outofsight-over-imageset" type="String" filter="boss" />
		<record alias="combat-outofsight-pressed-imageset" type="String" filter="boss" />
		<record alias="many-counts" type="Bool" filter="boss" />
		<!-- =================================================================== -->

		<!-- ========================== 5  开始 ========================== -->
		<record alias="env2spawn" filter="airdash" ref="ZoneEnv2Spawn" />
		<!-- =================================================================== -->

		<!-- ========================== 6  开始 ========================== -->
		<record alias="env2" filter="env" ref="ZoneEnv2" />
		<!-- =================================================================== -->

		<!-- ======================= [7=content] 开始 ======================= -->
		<record alias="attraction" filter="attraction" type="TRef" />
		<record alias="attraction-position-x" type="Float32" filter="attraction" />
		<record alias="attraction-position-y" type="Float32" filter="attraction" />
		<!-- =================================================================== -->

		<!-- ======================== [8 =npc] 开始 ======================== -->
		<record alias="npc" ref="Npc" filter="npc-group" />
		<!-- =================================================================== -->

		<!-- ======================== [9 =] 开始 ======================== -->
		<record alias="env2spawn" filter="guild-battle-field-portal" ref="ZoneEnv2Spawn" />
		<record alias="disable-imageset" type="String" filter="guild-battle-field-portal" />
		<record alias="disable-over-imageset" type="String" filter="guild-battle-field-portal" />
		<record alias="disable-pressed-imageset" type="String" filter="guild-battle-field-portal" />
		<record alias="disable-outofsight-imageset" type="String" filter="guild-battle-field-portal" />
		<record alias="disable-outofsight-over-imageset" type="String" filter="guild-battle-field-portal" />
		<record alias="disable-outofsight-pressed-imageset" type="String" filter="guild-battle-field-portal" />	
		<!-- =================================================================== -->

		<!-- ========================== 10 / 11 ========================== -->
		<record alias="enemy-side-imageset" type="String" filter="party-battle-startpoint-alpha" />
		<record alias="enemy-side-imageset" type="String" filter="party-battle-startpoint-beta" />
		<!-- =================================================================== -->
		
		
		<!-- ========================== 12  开始 ========================== -->
		<record alias="fishing-point-env" ref="ZoneEnv2" filter="fishing-field" />
		<!-- =================================================================== -->
	</list>
</table>