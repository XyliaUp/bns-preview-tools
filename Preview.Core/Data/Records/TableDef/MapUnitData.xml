			<case alias="none" default="y" />
			<case alias="player" />
			<case alias="party" />
			<case alias="team" />
			<case alias="guild" />
			<case alias="friend" />
			<case alias="revenge-enemy" />
			<case alias="faction" />
			<case alias="duel-enemy" />
			<case alias="quest" />
			<case alias="npc" />
			<case alias="env" />
			<case alias="teleport" />
			<case alias="airdash" />
			<case alias="link" />
			<case alias="convoy" />
			<case alias="spawned-env" />
			<case alias="static" />
			<case alias="auction" />
			<case alias="store" />
			<case alias="camp" />
			<case alias="party-camp" />
			<case alias="roulette" />
			<case alias="field-boss" />
			<case alias="gather" />
			<case alias="craft" />
			<case alias="gather-env" />
			<case alias="heart" />
			<case alias="enter-arena" />
