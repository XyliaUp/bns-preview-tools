		<record alias="tendency-1-faction2-warfare-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-2-faction1-warfare-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-2-faction2-warfare-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-3-faction1-warfare-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-3-faction2-warfare-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-1-faction1-sub-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-1-faction2-sub-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-2-faction1-sub-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-2-faction2-sub-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-3-faction1-sub-quest" ref="Quest" filter="buyudo" repeat="5" />
		<record alias="tendency-3-faction2-sub-quest" ref="Quest" filter="buyudo" repeat="5" />

		<record alias="faction1-kill-quest" ref="Quest" filter="buyudo" />
		<record alias="faction2-kill-quest" ref="Quest" filter="buyudo" />
		<record alias="defeat-field-play-point-bonus-percent" type="Int32" filter="buyudo" />
		<record alias="defeat-faction-score-bonus-percent" type="Int32" filter="buyudo" />
		<record alias="defeat-bonus-point-percent" type="Int32" filter="buyudo" />
		<record alias="reentrance-faction1-spawn" ref="ZonePcSpawn" filter="buyudo" />
		<record alias="reentrance-faction2-spawn" ref="ZonePcSpawn" filter="buyudo" />
		<record alias="warfare-draw-simple-quest-pack-play-section" type="Int8" filter="buyudo" client="n" repeat="2" />
		<record alias="warfare-defeat-faction-light-user-simple-quest-pack-play-section" type="Int8" filter="buyudo" client="n" repeat="2" />
		<record alias="warfare-defeat-faction-hard-user-simple-quest-pack-play-section" type="Int8" filter="buyudo" client="n" repeat="2" />
		<record alias="warfare-victory-faction-light-user-simple-quest-pack-play-section" type="Int8" filter="buyudo" client="n" repeat="2" />
		<record alias="warfare-victory-faction-hard-user-simple-quest-pack-play-section" type="Int8" filter="buyudo" client="n" repeat="2" />
		<record alias="victory-npc-reset-stage-tendency" ref="NpcResetStage" filter="buyudo" client="n" repeat="2" />
		<record alias="defeat-faction-separation-faction-score" ref="NpcResetStage" filter="buyudo" client="n" />
		<record alias="victory-faction-separation-faction-score" ref="NpcResetStage" filter="buyudo" client="n" />	
	</list>
</table>