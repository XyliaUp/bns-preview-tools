<?xml version="1.0" encoding="utf-8"?>
<table>
	<list module="ZoneData0" type="tendency-field" version="0.22" >
		<record alias="id" type="Int32" key="y" />
		<record alias="type" type="type" >
		     <case alias="normal" />
		     <case alias="buyudo" />		 
		</record>


		<record alias="alias" type="String" />
		<record alias="main-zone" ref="Zone" />
		<record alias="sub-zone" ref="Zone" repeat="7" />

		<record alias="enable-change-channel" type="Bool" />
		<record alias="required-level" type="Int8" />
		<record alias="required-mastery-level" type="Int8" />
		<record alias="required-preceding-quest" ref="Quest" repeat="3" />
		<record alias="required-preceding-quest-check" type="Seq" >
			<case alias="and" default="y" />
			<case alias="or" />
		</record>
		<record alias="required-activated-main-faction" type="Bool" />
		<record alias="restrict-enter-zone-when-manifest-tendency" type="Bool" />
		<record alias="stand-by-duration-minute" type="Int16" />
		<record alias="in-progress-duration-minute" type="Msec" />
		<record alias="tendency-1-max-play-value" type="Int8" default="1" />
		<record alias="tendency-2-max-play-value" type="Int8" default="1" />
		<record alias="tendency-3-max-play-value" type="Int8" default="1" />
		
		<record alias="tendency-reset-score-type" type="dictionary" >
			<case alias="none" />
			<case alias="tendency-1" />
			<case alias="tendency-2" />
			<case alias="tendency-3" />
			<case alias="tendency-1-2" />
			<case alias="tendency-1-3" />
			<case alias="tendency-2-3" />
			<case alias="tendency-1-2-3" default="y" />
		</record>
		
		<record alias="tendency-1-reset-score-type" type="Seq" seq="tendency-reset-score-type" />
		<record alias="tendency-2-reset-score-type" type="Seq" seq="tendency-reset-score-type" />
		<record alias="tendency-3-reset-score-type" type="Seq" seq="tendency-reset-score-type" />
		<record alias="quest-pack-simple-quest-refresh-money" type="Int64" />
		<record alias="weekly-time-table-for-added-reward-1" ref="WeeklyTimeTable" />
		<record alias="weekly-time-table-for-added-reward-2" ref="WeeklyTimeTable" />	
		<record alias="weekly-bonus-point-percent-1" type="Int32" />
		<record alias="weekly-bonus-point-percent-2" type="Int32" />		
		<record alias="group" ref="AttractionGroup" />
		<record alias="enable-infinite-hyper-energy" type="Bool" />
		<record alias="disable-move-to-arena" type="Bool" />
		<record alias="disable-move-to-skill-training-room" type="Bool" />
		<record alias="enable-change-faction-costume" type="Bool" default="y" />
		<record alias="enable-set-guild-meeting-place" type="Bool" default="y" />
		<record alias="enable-warp-to-guild-member" type="Bool" default="y" />
		<record alias="tendency-start-kismet-name" type="String" repeat="3" />
		<record alias="tendency-end-kismet-name" type="String" repeat="3" />	
		<record alias="tendency-field-name2" ref="Text" />
		<record alias="tendency-field-desc" ref="Text" />
		<record alias="reward-summary" ref="AttractionRewardSummary" />
		<record alias="ui-text-grade" type="Int8" />
		<record alias="tendency-standby-msg" ref="GameMessage" repeat="3" />
		<record alias="tendency-start-msg" ref="GameMessage" repeat="3" />	
		<record alias="tendency-timeup-alarm-msg" ref="GameMessage" repeat="3" />
		<record alias="tendency-end-msg" ref="GameMessage" repeat="3" />	
		<record alias="guide-standby-msg" ref="GameMessage" repeat="3" />
