		<record alias="quest-pack-simple-quest-explicit-refresh-bonus-multiple" type="Int8" client="n" repeat="6" />
		<record alias="quest-pack-simple-quest-implicit-refresh-bonus-prob" type="Int8" client="n" repeat="6" />		
		<record alias="quest-pack-simple-quest-implicit-refresh-bonus-multiple" type="Int8" client="n" repeat="6" />
		<record alias="tendency-1-npc-reset-stage-start" ref="NpcResetStage" client="n" repeat="5" />
		<record alias="tendency-2-npc-reset-stage-start" ref="NpcResetStage" client="n" repeat="5" />
		<record alias="tendency-3-npc-reset-stage-start" ref="NpcResetStage" client="n" repeat="5" />
		<record alias="tendency-1-npc-reset-stage-finish" ref="NpcResetStage" client="n" repeat="5" />
		<record alias="tendency-2-npc-reset-stage-finish" ref="NpcResetStage" client="n" repeat="5" />
		<record alias="tendency-3-npc-reset-stage-finish" ref="NpcResetStage" client="n" repeat="5" />
		<!-- ================================================================== -->
		<record alias="playable-time" ref="WeeklyTimeTable" filter="buyudo" />
		<record alias="check-score-required-min-faction-member-count" type="Int16" default="6" filter="buyudo" />
		<record alias="tendency-1-faction1-warfare-boss" ref="Npc" filter="buyudo" repeat="5" />
		<record alias="tendency-1-faction2-warfare-boss" ref="Npc" filter="buyudo" repeat="5" />
		<record alias="tendency-2-faction1-warfare-boss" ref="Npc" filter="buyudo" repeat="5" />
		<record alias="tendency-2-faction2-warfare-boss" ref="Npc" filter="buyudo" repeat="5" />
		<record alias="tendency-3-warfare-boss" ref="Npc" filter="buyudo" repeat="5" />

