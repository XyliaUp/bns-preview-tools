		<record alias="damage-power-percent-modify-diff" type="Int32" />
		<record alias="hp-drain-percent-modify-percent" type="Int16" />
		<record alias="hp-drain-percent-modify-diff" type="Int32" />
		<record alias="heal-percent-modify-percent" type="Int16" />
		<record alias="heal-percent-modify-diff" type="Int32" />
