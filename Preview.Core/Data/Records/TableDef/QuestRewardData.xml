		<record alias="id" type="Int32" key="y" />
		<record alias="alias" type="String" />
		<record alias="quest-first-progress" type="Seq" >
			<case alias="none" default="y" />
			<case alias="y" />
			<case alias="n" />
		</record>
		<record alias="quest-completion-count" type="Int8" />
		<record alias="quest-completion-count-op" type="Int8" />
		<record alias="basic-money" type="Int32" />
