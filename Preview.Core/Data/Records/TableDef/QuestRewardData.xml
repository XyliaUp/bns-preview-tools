		<record alias="optional-1-" struct="optional-group" />
		<record alias="optional-2-" struct="optional-group" />
		<record alias="optional-3-" struct="optional-group" />
		<record alias="optional-4-" struct="optional-group" />
		<record alias="optional-5-" struct="optional-group" />
		<record alias="optional-6-" struct="optional-group" />
