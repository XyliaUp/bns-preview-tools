		<record alias="optional-group" type="struct" >
			<record alias="faction" ref="Faction" />
			<record alias="job" seq="job" type="Seq" repeat="16"/>
			<record alias="sex" seq="sex" type="Seq" repeat="4"/>
			<record alias="race" seq="race" type="Seq" repeat="4"/>
			<record alias="difficulty-type" seq="difficulty-type" type="Seq" />
			<record alias="slot" type="TRef" repeat="4"/>
			<record alias="item-count" type="Int8" repeat="4" default="1" />
		</record>
		<record alias="fixed-group" type="struct" parent="optional-group" >
			<record alias="skill-var-idx" type="Int8" repeat="4" />
		</record>

		<record alias="fixed-1-" struct="fixed-group" />
