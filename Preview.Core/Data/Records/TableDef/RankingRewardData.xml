<?xml version="1.0" encoding="utf-8"?>
<table>
  <list module="ItemData" server-side="rank | server" type="RankingReward" version="0.4" >
    <record alias="alias" type="String" />
    <!-- ===================================================== -->
    <record alias="week-reward-exp" type="Int32" />
    <record alias="week-reward-money" show-zero="y" type="Int32" />
    <record alias="week-reward-item" ref="Item" name="周奖励物品" repeat="4" />
    <record alias="week-reward-item-amount" name="周奖励物品数量" type="Int32" repeat="4" />
    <!-- ===================================================== -->
    <record alias="season-reward-exp" type="Int32" />
    <record alias="season-reward-money" show-zero="y" type="Int32" />
    <record alias="season-reward-item" ref="Item" name="赛季奖励物品" repeat="4" />
    <record alias="season-reward-item-amount" name="赛季奖励物品数量" type="Int32" repeat="4" />
    <!-- ===================================================== -->
    <record alias="week-reward-combat-exp-bonus-rate" type="Int16" />
    <record alias="week-reward-duel-point-bonus-rate" type="Int16" />
    <record alias="week-reward-party-battle-point-bonus-rate" type="Int16" />
    <record alias="week-reward-field-point-bonus-rate" type="Int16" />
    <record alias="season-reward-combat-exp-bonus-rate" type="Int16" />
    <record alias="season-reward-duel-point-bonus-rate" type="Int16" />
    <record alias="season-reward-party-battle-point-bonus-rate" type="Int16" />
    <record alias="season-reward-field-point-bonus-rate" type="Int16" />
    <!-- ===================================================== -->
    <record alias="season-reward-guild-bank-money" type="Int32" />
    <record alias="season-reward-guild-bank-item" type="Seq" repeat="5" >
		<case alias="none" default="y" />
		<case alias="gemstone" />
		<case alias="moonstone" />
		<case alias="tokeniron" />
		<case alias="tokensilver" />
		<case alias="tokengold" />
	</record>    
    <record alias="season-reward-guild-bank-item-amount" type="Int32" repeat="5" />
    <record alias="guild-ranking-reward-effect" ref="Effect" />
  </list>
</table>