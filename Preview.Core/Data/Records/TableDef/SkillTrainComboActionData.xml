<?xml version="1.0" encoding="utf-8"?>
<table>
	<list module="CombatData" type="skill-train-combo-action" version="0.5" >
		<record alias="job" type="Seq" seq="job" key="y" />
		<record alias="job-style" type="Seq" seq="job-style" key="y" />
		<record alias="stance-index" type="Int8" key="y" />
		<record alias="buff-index" type="Int8" key="y" />
		<record alias="function-index" type="Int8" key="y" />

		<record alias="alias" type="String" />
		<record alias="stance-name" ref="Text" />
		<record alias="buff-name" ref="Text" />
		<record alias="function-name" ref="Text" />
		<record alias="function-desc" ref="Text" />
		<record alias="combo-skill" type="Seq" repeat="10" >
			<case alias="red" default="y" />
			<case alias="blue" />
			<case alias="green" />
			<case alias="yellow" />
		</record>
		<record alias="skill-id" type="Int32" repeat="10" />
		<record alias="variation-id" type="Int8" repeat="10" />
		<record alias="condition-icon-normal-text" ref="Text" repeat="10" />
		<record alias="condition-icon-over-text" ref="Text" repeat="10" />
		<record alias="condition-tooltip-text" ref="Text" repeat="10" />
	</list>
</table>