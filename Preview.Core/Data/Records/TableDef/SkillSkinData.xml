<?xml version="1.0" encoding="utf-8"?>
<table>
	<list module="ItemData" type="SkillSkin" version="1.0" >
		<record alias="id" key="y" type="Int16" />
		
		<record alias="alias" type="String" />
		<record alias="skill-skin-type" type="Seq" >
			<case alias="none" />
			<case alias="use-grocery" />
			<case alias="equip-item" />
			<case alias="equip-set-item" />
		</record>
		<record alias="name" ref="Text" />
		<record alias="icon" type="Icon" />
		<record alias="iteminfo" ref="Item" repeat="20" server="n" />
		<record alias="grade" type="Int8" default="1" server="n" />
		<record alias="skill-skin-play-bink" type="String" repeat="20" server="n" />
		<record alias="background-image" type="String" server="n" />
	</list>
</table>