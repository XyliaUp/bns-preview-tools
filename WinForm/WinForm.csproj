	<ItemGroup>
		<Content Include="Resources\Game.xml" />
		<Content Include="Properties\Query.ico" />
		<Content Include="Resources\Tips.xml" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="CUE4Parse" Version="1.0.9" />
		<PackageReference Include="HtmlAgilityPack" Version="1.11.46" />
		<PackageReference Include="HZH_Controls" Version="2.0.0.3" />
		<PackageReference Include="Xylia" Version="1.0.3.15" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\BNSBIN\Src\BnsBinTool.Core\BnsBinTool.Core.csproj" />
		<ProjectReference Include="..\Preview.Core\Preview.Core.csproj" />
		<ProjectReference Include="..\Preview.UI\Preview.UI.csproj" />
	</ItemGroup>
	<ItemGroup>
		<EmbeddedResource Update="Properties\Resources.resx">
			<Generator>PublicResXFileCodeGenerator</Generator>
			<LastGenOutput>Resources.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>
	<ItemGroup>
		<None Update="Properties\Query.ico">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Update="Resources\Attribute.xml">
		  <Generator></Generator>
		</None>
	</ItemGroup>
	<ProjectExtensions><VisualStudio><UserProperties program_1deps_1json__JsonSchema="https://beaujs.com/schema.json" /></VisualStudio></ProjectExtensions>
